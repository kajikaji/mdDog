<section class="Message">
  [%IF isExistBuffer %]
  <div class="IsExistBuffer">
    バッファがコミットされていません
    <a class="Btn">バッファリセット</a>

  </div>
  [%END %]

  [%IF message.error %]
    <ul class="Error">
    [%FOREACH e IN message.error %]
      <li>[% e %]</li>
    [%END %]
    </ul>
  [%END %]
  [%IF message.info %]
    <ul class="Info">
    [%FOREACH i IN  message.info %]
      <li>[% i %]</li>
    [%END %]
    </ul>
  [%END %]
</section>
